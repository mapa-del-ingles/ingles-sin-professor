      window.addEventListener('pageLazyLoad', function () {
        var date = new Date();
        var timezoneOffset = (date.getTimezoneOffset() / 60) * 60 * 60 * 1000;
        var neutralDate = new Date(date.getTime() - timezoneOffset);
        window.dispatchEvent(
          new CustomEvent('timerStartDate', { detail: neutralDate.getTime() })
        );
        
    setInterval(function () {
      window.dispatchEvent(new CustomEvent('timerTick'));
    }, 1000);
  
